<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="Viabo\catalogs\threshold\domain\Threshold" table="cat_threshold_types">
        <embedded name="id" class="ThresholdId" use-column-prefix="false"/>
        <embedded name="name" class="ThresholdName" use-column-prefix="false"/>
        <embedded name="value" class="ThresholdValue" use-column-prefix="false"/>
        <embedded name="active" class="ThresholdActive" use-column-prefix="false"/>
    </entity>

</doctrine-mapping>