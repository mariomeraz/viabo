<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="Viabo\management\billing\domain\Billing" table="t_management_billing_spei">
        <embedded name="id" class="BillingId" use-column-prefix="false"/>
        <embedded name="reference" class="BillingReference" use-column-prefix="false"/>
        <embedded name="apiKey" class="BillingApiKey" use-column-prefix="false"/>
        <embedded name="commissionType" class="BillingCommissionType" use-column-prefix="false"/>
        <embedded name="cardNumber" class="BillingCardNumber" use-column-prefix="false"/>
        <embedded name="percentage" class="BillingCommissionPercentage" use-column-prefix="false"/>
        <embedded name="amount" class="BillingAmount" use-column-prefix="false"/>
        <embedded name="commission" class="BillingCommission" use-column-prefix="false"/>
        <embedded name="data" class="BillingData" use-column-prefix="false"/>
        <embedded name="registerDate" class="BillingRegisterDate" use-column-prefix="false"/>
    </entity>

</doctrine-mapping>